{
    "manifest_version": 2,
    "name": "Hello World",
    "description": "My first extension for chrome",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "style.css"
            ],
            "icon": [
                "cine.png"
            ]
        }    
    ],

    "browser_action": {
           "default_icon": "icon.png",
           "default_popup": "index.html"
    }
}