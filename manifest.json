{
    "manifest_version": 2,
    "name": "Youtube-Manager",
    "description": "noAds and download videos on youtube!",
    "version": "0.2",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "style.css"
            ],
            "icon": [
                "cine.png"
            ]
        }    
    ],

    "browser_action": {
            "default_title": "Youtube-manager",
           "default_icon": "icon.png",
           "default_popup": "index.html"
    }
}